<p id="notice"><%= notice %></p>

<h1>Freelance Documents</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>File url</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @freelance_documents.each do |freelance_document| %>
      <tr>
        <td><%= freelance_document.title %></td>
        <td><%= freelance_document.description %></td>
        <td><%= freelance_document.file_url %></td>
        <td><%= freelance_document.image_url %></td>
        <td><%= link_to 'Show', freelance_document %></td>
        <td><%= link_to 'Edit', edit_freelance_document_path(freelance_document) %></td>
        <td><%= link_to 'Destroy', freelance_document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Freelance Document', new_freelance_document_path %>
